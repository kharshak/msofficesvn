<?xml version="1.0" encoding="Shift-JIS"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="Subversion" label="Subversion">
				<group id="SVNGroup" label="Subversion">
					<button id="Update" label="更新" imageMso="FileCheckOut" size="large"  onAction="UpdateOnAct" />
					<button id="Lock" label="ロックを取得" imageMso="Lock" size="large" onAction="LockOnAct" />
					<button id="Commit" label="コミット" imageMso="FileCheckIn" size="large" onAction="CommitOnAct" />
					<button id="Diff" label="差分" imageMso="ReviewCompareTwoVersions" size="large"  onAction="DiffOnAct" />
					<button id="Log" label="ログ表示" imageMso="ReviewTrackChanges" size="large"  onAction="LogOnAct" />
					<button id="RepoBrowser" label="リポジトリブラウザ" imageMso="LookUp" size="large"  onAction="RepoBrowserOnAct" />
					<button id="UnLock" label="ロックを開放" imageMso="AdpPrimaryKey" onAction="UnLockOnAct" />
					<button id="Add" label="追加" imageMso="OutlineExpand" onAction="AddOnAct" />
					<button id="Delete" label="削除" imageMso="Delete" onAction="DeleteOnAct" />
				</group>
				<group id="File" label="ファイル">
					<button id="Explorer" label="エクスプローラを開く" imageMso="FileOpen" size="large"  onAction="OpenExplorerOnAct" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
