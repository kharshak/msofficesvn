<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="Subversion" label="Subversion">
				<group id="SVNGroup" label="Subversion">
					<button id="Update" label="Update" imageMso="FileCheckOut" size="large"  onAction="UpdateOnAct" />
					<button id="Lock" label="Lock" imageMso="Lock" size="large" onAction="LockOnAct" />
					<button id="Commit" label="Commit" imageMso="FileCheckIn" size="large" onAction="CommitOnAct" />
					<button id="Diff" label="Diff" imageMso="ReviewCompareTwoVersions" size="large"  onAction="DiffOnAct" />
					<button id="Log" label="Log" imageMso="ReviewTrackChanges" size="large"  onAction="LogOnAct" />
					<button id="RepoBrowser" label="RepoBrowser" imageMso="LookUp" size="large"  onAction="RepoBrowserOnAct" />
					<button id="UnLock" label="UnLock" imageMso="AdpPrimaryKey" onAction="UnLockOnAct" />
					<button id="Add" label="Add" imageMso="OutlineExpand" onAction="AddOnAct" />
					<button id="Delete" label="Delete" imageMso="Delete" onAction="DeleteOnAct" />
				</group>
				<group id="File" label="File">
					<button id="Explorer" label="Explorer" imageMso="FileOpen" size="large"  onAction="OpenExplorerOnAct" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
